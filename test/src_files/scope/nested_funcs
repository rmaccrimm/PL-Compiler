$ Redefine variables in nested functions and re-use func name - 3 errors
begin
    proc f
    begin
        proc g
        begin 
            proc h
            begin
                call f;
                call g;
                call h;
            end;

            proc f 
            begin 
                integer g;
                call f;
                call g; $ 2 errors - type error and scope error
                call h; 
            end;

            call h;
            call f;
            call g;
        end;

        call f;
        call h; $ 1 error
    end;
end.